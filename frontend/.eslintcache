ading: articlesLoading,\n    error: articlesError,\n  } = useRequest(`/api/stocks/${symbol}/symbol-headlines`);\n\n  // const transactionList = useSelector((state) => state.transactionList);\n\n  const {\n    userLogin: { userInfo },\n  } = useSelector((state) => state.userLogin);\n\n  const { loading, data, error } = useRequest(\n    `/api/stocks/${symbol}`,\n    userInfo\n  );\n\n  const transactionDelete = useSelector((state) => state.transactionDelete);\n  const { success } = transactionDelete;\n\n  useEffect(() => {\n    dispatch(listTransactions(symbol));\n  }, [dispatch, symbol, success]);\n\n  const deleteHandler = (transactionId) => {\n    if (window.confirm('Are you sure?')) {\n      dispatch(deleteTransaction(transactionId));\n    }\n  };\n\n  return (\n    <>\n      <h1 className='text-center'>{symbol}</h1>\n      <Row>\n        <TradingViewWidget\n          className='trading-view-graph'\n          symbol={symbol}\n          style='2'\n        />\n      </Row>\n      {loading && <Loader />}\n      {error && <Message>{error}</Message>}\n      {data.id && (\n        <>\n          <Row className='my-5'>\n            <Col>\n              <div className='text-center border border-success mx-5 rounded'>\n                <p>Price</p>\n                <h3>${data.price}</h3>\n              </div>\n            </Col>\n            <Col>\n              <div className='text-center border border-success mx-5 rounded'>\n                <p>Total Shares</p>\n                <h3>{data.totalShares}</h3>\n              </div>\n            </Col>\n          </Row>\n          <h2>Transactions</h2>\n          {data.transactions.length ? (\n            <Table striped bordered hover responsive className='table-sm'>\n              <thead>\n                <tr className='table-primary'>\n                  <th>Symbol</th>\n                  <th>Shares</th>\n                  <th>Price</th>\n                  <th>Type</th>\n                  <th></th>\n                </tr>\n              </thead>\n              <tbody>\n                {data.transactions.map((transaction) => (\n                  <tr key={transaction.transactionid}>\n                    <td>{transaction.symbol}</td>\n                    <td>{transaction.shares}</td>\n                    <td>{transaction.avg_price}</td>\n                    <td>{transaction.type}</td>\n                    <td>\n                      <LinkContainer\n                        to={`/transaction/${transaction.transactionid}/edit`}>\n                        <Button variant='warning' className='btn-sm mx-2'>\n                          Update\n                        </Button>\n                      </LinkContainer>\n\n                      <Button\n                        onClick={() => deleteHandler(transaction.transactionid)}\n                        variant='danger'\n                        className='btn-sm'>\n                        Delete\n                      </Button>\n                    </td>\n                  </tr>\n                ))}\n              </tbody>\n            </Table>\n          ) : (\n            <Message variant='warning'>\n              There are no transactions for this symbol\n            </Message>\n          )}\n          <h2>Related News</h2>\n          <CardColumns>\n            {articlesLoading && <Loader />}\n            {articlesError && <Message>{articlesError}</Message>}\n            {Object.entries(articles).length !== 0 ? (\n              articles.news.slice(0, 6).map((story, index) => (\n                <Card key={index} style={{ height: '28rem' }}>\n                  <Card.Img\n                    variant='top'\n                    style={{ maxHeight: '13rem' }}\n                    src={story.urlToImage}\n                  />\n                  <Card.Body>\n                    <Card.Title>{story.title}</Card.Title>\n                    <Card.Text>\n                      {story.content.substring(0, 120) + '...'}\n                    </Card.Text>\n                    <a href={story.url}>\n                      <Button variant='success'>Read More</Button>\n                    </a>\n                  </Card.Body>\n                </Card>\n              ))\n            ) : (\n              <p>There are no stories for this stock</p>\n            )}\n          </CardColumns>\n        </>\n      )}\n    </>\n  );\n};\n\nexport default StockDetailsScreen;\n","/Users/vinicius/Desktop/Web Dev/react-redux-stock-tracker/frontend/src/components/IndexCard.js",["103","104"],"import React from 'react';\nimport { Col } from 'react-bootstrap';\nimport { LinkContainer } from 'react-router-bootstrap';\n\nconst IndexCard = ({\n  symbol,\n  price,\n  nominalVariation,\n  percentagePriceVariation,\n}) => {\n  return (\n    <Col xs={12} md={6} lg={3} className='index-card__col'>\n      <div className='index-card'>\n        <LinkContainer to={`/stock/${symbol}`}>\n          <a href='/'>{symbol}</a>\n        </LinkContainer>\n        <div>\n          <h5 className='index-card__price mx-2'>{price}</h5>\n          <h6 className='index-card__performance mx-2'>\n            {nominalVariation > 0 ? (\n              <span className='index-card__performance--positive'>\n                {'+' + nominalVariation} <i className='fas fa-caret-up'></i>{' '}\n                {'(' + percentagePriceVariation + '%' + ')'}\n              </span>\n            ) : (\n              <span className='index-card__performance--negative'>\n                {nominalVariation} <i className='fas fa-caret-down'></i>{' '}\n                {'(' + percentagePriceVariation + '%' + ')'}\n              </span>\n            )}\n          </h6>\n        </div>\n      </div>\n    </Col>\n  );\n};\n\nexport default IndexCard;\n","/Users/vinicius/Desktop/Web Dev/react-redux-stock-tracker/frontend/src/hooks/useRequest.js",[],{"ruleId":"105","replacedBy":"106"},{"ruleId":"107","replacedBy":"108"},{"ruleId":"105","replacedBy":"109"},{"ruleId":"107","replacedBy":"110"},{"ruleId":"111","seve